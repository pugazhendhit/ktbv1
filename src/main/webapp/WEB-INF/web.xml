<?xml version="1.0" encoding="UTF-8"?>
<web-app
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
  
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <display-name>ProcessHandler</display-name>
  	<servlet-name>ProcessHandler</servlet-name>
  	<servlet-class>com.cloudasset.sample.ProcessHandler</servlet-class>
  </servlet>
  <servlet>
    <display-name>GetStatus</display-name>
  	<servlet-name>GetStatus</servlet-name>
  	<servlet-class>com.cloudasset.sample.GetStatus</servlet-class>
  </servlet>
  
  <servlet>
    <display-name>Status</display-name>
  	<servlet-name>Status</servlet-name>
  	<servlet-class>com.cloudasset.sample.Status</servlet-class>
  </servlet>
  
  <servlet>
    <display-name>LoginSevlet</display-name>
  	<servlet-name>LoginSevlet</servlet-name>
  	<servlet-class>com.cloudasset.sample.LoginSevlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>RegisterServlet</display-name>
  	<servlet-name>RegisterServlet</servlet-name>
  	<servlet-class>com.cloudasset.sample.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>LogoutServlet</display-name>
  	<servlet-name>LogoutServlet</servlet-name>
  	<servlet-class>com.cloudasset.sample.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
   	<display-name>AddProduct</display-name>
  	<servlet-name>AddProduct</servlet-name>
  	<servlet-class>com.cloudasset.controller.AddProduct</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProcessHandler</servlet-name>
  	<url-pattern>/PayProcess.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>GetStatus</servlet-name>
  	<url-pattern>/getStatus.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Status</servlet-name>
  	<url-pattern>/status.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginSevlet</servlet-name>
  	<url-pattern>/LoginSevlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/LogoutServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddProduct</servlet-name>
  	<url-pattern>/AddProduct.do</url-pattern>
  </servlet-mapping>

</web-app>
